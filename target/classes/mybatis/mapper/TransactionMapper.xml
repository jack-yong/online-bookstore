<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyong.mapper.TransactionMapper">
        <insert id="addTrade" parameterType="Transaction">
                insert into onlinebookstore.transaction
                 (Bid,Oid,Uid,Tstatus,Tboughtnum,Tsubmittime) values
                 (#{Bid},#{Oid},#{Uid},#{Tstatus},#{Tboughtnum},#{Tsubmittime})
        </insert>
</mapper>